{
    "require": {
        "illuminate/database": "^7.11",
        "php-di/php-di": "^6.1",
        "php-di/slim-bridge": "^3.0",
        "respect/validation": "^2.0",
        "selective/config": "^0.2.0",
        "slim/psr7": "^1.1",
        "slim/slim": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
